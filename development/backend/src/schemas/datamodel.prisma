type Listing {
  id: ID! @unique
  title: String!
  description: String!
  price: Int!
  imgURL: String!
  category: Category
  createdBy: User
}

type User {
  id: ID! @unique
  password: String!
  firstName: String!
  surname: String!
  email: String!
  phone: String
}

type Category { 
  id: ID! @unique
  label: String!
}